{
	"type": "module",
	"scripts": {
		"release": "changelogen --dir packages/lib --release --push",
		"release:canary": "changelogen --dir packages/lib --canary && pnpm publish --filter @vuetils/vue-useform --no-git-checks --tag canary",
		"format": "prettier --write . ",
		"format:check": "prettier --check ."
	},
	"devDependencies": {
		"changelogen": "^0.5.4",
		"prettier": "^3.0.0",
		"prettier-plugin-organize-imports": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.4.1"
	}
}
