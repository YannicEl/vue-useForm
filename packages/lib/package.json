  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:types": "vue-tsc --emitDeclarationOnly --noEmit false",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^18.17.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vuetils/form": "workspace:latest",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vitest": "^0.34.1",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  },
  "peerDependencies": {
    "vue": "3.x"
  }
